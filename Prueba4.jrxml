<?xml version="1.0" encoding="UTF-8"?>
<jasperReport
    xmlns="http://jasperreports.sourceforge.net/jasperreports"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
    name="Prueba2"
    pageWidth="595"
    pageHeight="842"
    columnWidth="555"
    leftMargin="20"
    rightMargin="20"
    topMargin="20"
    bottomMargin="20"
>

    <parameter name="Tabla_de_Reservas" class="java.lang.String"/>

    <!-- Query con los nombres EXACTOS de las columnas -->
    <queryString>
        <![CDATA[
        SELECT 
            NumExpediente,
            FechaEntrada,
            TiempoEstancia,
            NombreCliente,
            ApellidosCliente,
            DNI,
            NumIdentificador,
            M2,
            Precio
        FROM reservas
        ]]>
    </queryString>

    <!-- Campos con los nombres EXACTOS de las columnas -->
    <field name="NumExpediente" class="java.lang.String"/>
    <field name="FechaEntrada" class="java.util.Date"/>
    <field name="TiempoEstancia" class="java.lang.Integer"/>
    <field name="NombreCliente" class="java.lang.String"/>
    <field name="ApellidosCliente" class="java.lang.String"/>
    <field name="DNI" class="java.lang.String"/>
    <field name="NumIdentificador" class="java.lang.String"/>
    <field name="M2" class="java.lang.Integer"/>
    <field name="Precio" class="java.math.BigDecimal"/>

    <!-- TÃ­tulo -->
    <title>
        <band height="70">
            <staticText>
                <reportElement x="0" y="0" width="555" height="30"/>
                <textElement textAlignment="Center">
                    <font size="18" isBold="true"/>
                </textElement>
                <text><![CDATA[REPORTE DE RESERVAS]]></text>
            </staticText>

            <textField>
                <reportElement x="0" y="40" width="555" height="20"/>
                <textElement textAlignment="Center"/>
                <textFieldExpression><![CDATA[$P{Tabla_de_Reservas}]]></textFieldExpression>
            </textField>
        </band>
    </title>

    <!-- Cabecera de columnas -->
    <columnHeader>
        <band height="30">
            <staticText><reportElement x="0" y="0" width="60" height="30" backcolor="#E0E0E0"/><textElement verticalAlignment="Middle"><font isBold="true"/></textElement><text><![CDATA[EXPEDIENTE]]></text></staticText>
            <staticText><reportElement x="60" y="0" width="70" height="30" backcolor="#E0E0E0"/><textElement verticalAlignment="Middle"><font isBold="true"/></textElement><text><![CDATA[FECHA]]></text></staticText>
            <staticText><reportElement x="130" y="0" width="50" height="30" backcolor="#E0E0E0"/><textElement verticalAlignment="Middle"><font isBold="true"/></textElement><text><![CDATA[ESTANCIA]]></text></staticText>
            <staticText><reportElement x="180" y="0" width="70" height="30" backcolor="#E0E0E0"/><textElement verticalAlignment="Middle"><font isBold="true"/></textElement><text><![CDATA[NOMBRE]]></text></staticText>
            <staticText><reportElement x="250" y="0" width="70" height="30" backcolor="#E0E0E0"/><textElement verticalAlignment="Middle"><font isBold="true"/></textElement><text><![CDATA[APELLIDOS]]></text></staticText>
            <staticText><reportElement x="320" y="0" width="60" height="30" backcolor="#E0E0E0"/><textElement verticalAlignment="Middle"><font isBold="true"/></textElement><text><![CDATA[DNI]]></text></staticText>
            <staticText><reportElement x="380" y="0" width="50" height="30" backcolor="#E0E0E0"/><textElement verticalAlignment="Middle"><font isBold="true"/></textElement><text><![CDATA[ID]]></text></staticText>
            <staticText><reportElement x="430" y="0" width="50" height="30" backcolor="#E0E0E0"/><textElement verticalAlignment="Middle"><font isBold="true"/></textElement><text><![CDATA[M2]]></text></staticText>
            <staticText><reportElement x="480" y="0" width="75" height="30" backcolor="#E0E0E0"/><textElement verticalAlignment="Middle"><font isBold="true"/></textElement><text><![CDATA[PRECIO]]></text></staticText>
        </band>
    </columnHeader>

    <!-- Detalle CORREGIDO con nombres exactos -->
    <detail>
        <band height="25">
            <textField><reportElement x="0" y="0" width="60" height="25"/><textElement verticalAlignment="Middle"/><textFieldExpression><![CDATA[$F{NumExpediente}]]></textFieldExpression></textField>
            <textField pattern="dd/MM/yyyy"><reportElement x="60" y="0" width="70" height="25"/><textElement verticalAlignment="Middle"/><textFieldExpression><![CDATA[$F{FechaEntrada}]]></textFieldExpression></textField>
            <textField><reportElement x="130" y="0" width="50" height="25"/><textElement verticalAlignment="Middle"/><textFieldExpression><![CDATA[$F{TiempoEstancia}]]></textFieldExpression></textField>
            <textField><reportElement x="180" y="0" width="70" height="25"/><textElement verticalAlignment="Middle"/><textFieldExpression><![CDATA[$F{NombreCliente}]]></textFieldExpression></textField>
            <textField><reportElement x="250" y="0" width="70" height="25"/><textElement verticalAlignment="Middle"/><textFieldExpression><![CDATA[$F{ApellidosCliente}]]></textFieldExpression></textField>
            <textField><reportElement x="320" y="0" width="60" height="25"/><textElement verticalAlignment="Middle"/><textFieldExpression><![CDATA[$F{DNI}]]></textFieldExpression></textField>
            <textField><reportElement x="380" y="0" width="50" height="25"/><textElement verticalAlignment="Middle"/><textFieldExpression><![CDATA[$F{NumIdentificador}]]></textFieldExpression></textField>
            <textField><reportElement x="430" y="0" width="50" height="25"/><textElement verticalAlignment="Middle"/><textFieldExpression><![CDATA[$F{M2}]]></textFieldExpression></textField>
            <textField pattern="#,##0.00"><reportElement x="480" y="0" width="75" height="25"/><textElement verticalAlignment="Middle"/><textFieldExpression><![CDATA[$F{Precio}]]></textFieldExpression></textField>
        </band>
    </detail>

</jasperReport>